            </intent-filter>
        </receiver>
        <receiver android:name="com.angcyo.receiver.Receiver2">
            <intent-filter>
                <action android:name="android.intent.action.MEDIA_UNMOUNTED" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.angcyo.receiver.Receiver3">
