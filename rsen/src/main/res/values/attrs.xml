<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HighlightTextView">
        <attr name="highlight_position" format="integer" />
        <attr name="highlight_num" format="integer" />
        <attr name="highlight_color" format="color" />
    </declare-styleable>

    <declare-styleable name="MultiImageViewGroup">
        <attr name="image_num" format="integer" />
        <attr name="default_img" format="reference" />
        <attr name="horizontal_spacing" format="dimension" />
        <attr name="image_width" format="dimension" />
        <attr name="image_height" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ControlEditText">
        <attr name="add_step" format="integer" />
        <attr name="sub_step" format="integer" />
        <attr name="max_value" format="integer" />
        <attr name="min_value" format="integer" />
        <attr name="button_weight" format="float" />
        <attr name="border_radii" format="dimension" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_color_press" format="color" />
        <attr name="show_error" format="boolean" />
        <attr name="error_text" format="string" />

    </declare-styleable>

    <item name="left_button" type="id" />
    <item name="right_button" type="id" />
    <item name="center_edit_text" type="id" />

    <declare-styleable name="TagRadioGroup">
        <attr name="childWidth" format="dimension">
            <enum name="fill_parent" value="-1" />
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="childHeight" format="dimension">
            <enum name="fill_parent" value="-1" />
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="childPadding" format="dimension" />
        <attr name="childPaddingLeft" format="dimension" />
        <attr name="childPaddingRight" format="dimension" />
        <attr name="childPaddingTop" format="dimension" />
        <attr name="childPaddingBottom" format="dimension" />
        <attr name="childTextSize" format="dimension" />
        <attr name="childTextColor" format="color" />
        <attr name="childTextColorPress" format="color" />
        <attr name="childBorderWidth" format="dimension" />
        <attr name="childRound" format="dimension" />
        <attr name="childCount" format="integer" />
        <attr name="checkButtonIndex" format="integer" />
    </declare-styleable>

    <declare-styleable name="PathButton_old">
        <attr name="pathOldOffset" format="dimension"/>
        <attr name="pathOldWidth" format="dimension"/>
        <attr name="pathOldColor" format="color"/>
        <attr name="pathOldDrawDelay" format="integer"/>
        <attr name="pathOldDrawStep" format="integer"/>
        <attr name="pathOldBorderStyle" format="boolean"/>
        <attr name="pathOldBorderWidth" format="dimension"/>
        <attr name="pathOldBorderRound" format="dimension"/>
        <attr name="pathOldBorderColor" format="color"/>

        <attr name="pathOldStartGravity" format="integer">
            <enum name="LEFT_TOP" value="1"/>
            <enum name="TOP" value="2"/>
            <enum name="RIGHT_TOP" value="3"/>
            <enum name="RIGHT" value="4"/>
            <enum name="RIGHT_BOTTOM" value="5"/>
            <enum name="BOTTOM" value="6"/>
            <enum name="LEFT_BOTTOM" value="7"/>
            <enum name="LEFT" value="8"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CirclePathButton">
        <attr name="circlePathOffset" format="dimension"/>
        <attr name="circlePathWidth" format="dimension"/>
        <attr name="circlePathColor" format="color"/>
        <attr name="circlePathSelectColor" format="color"/>
        <attr name="circlePathDrawDelay" format="integer"/>
        <attr name="circlePathDrawStep" format="integer"/>
        <attr name="circlePathBorderStyle" format="boolean"/>
        <attr name="circlePathSelectStyle" format="boolean"/>
        <attr name="circlePathBorderColor" format="color"/>
        <attr name="circlePathStartAngle" format="float"/>
    </declare-styleable>

    <declare-styleable name="LovePathProgress">
        <attr name="lovePathColor" format="color"/>
        <attr name="lovePathWidth" format="dimension"/>
        <attr name="lovePathDrawDelay" format="integer"/>
        <attr name="lovePathPauseTime" format="integer"/>
        <attr name="lovePathMinSize" format="dimension"/>
        <attr name="lovePathDrawStep" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BorderButton">
        <attr name="borderBtWidth" format="dimension"/>
        <attr name="borderBtRound" format="dimension"/>
        <attr name="borderBtColor" format="color"/>
        <attr name="borderBtColorPress" format="color"/>
        <attr name="borderBtColorBg" format="color"/>
        <attr name="borderBtColorBgPress" format="color"/>
    </declare-styleable>


    <declare-styleable name="PathButton">
        <attr name="pathWidth" format="dimension"/>
        <attr name="pathColor" format="color"/>
        <attr name="pathRound" format="dimension"/>

        <attr name="pathStartGravity" format="integer">
            <enum name="LEFT_TOP" value="1"/>
            <enum name="TOP" value="2"/>
            <enum name="RIGHT_TOP" value="3"/>
            <enum name="RIGHT" value="4"/>
            <enum name="RIGHT_BOTTOM" value="5"/>
            <enum name="BOTTOM" value="6"/>
            <enum name="LEFT_BOTTOM" value="7"/>
            <enum name="LEFT" value="8"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="indicator">
            <flag name="BallPulse" value="0"/>
            <flag name="BallGridPulse" value="1"/>
            <flag name="BallClipRotate" value="2"/>
            <flag name="BallClipRotatePulse" value="3"/>
            <flag name="SquareSpin" value="4"/>
            <flag name="BallClipRotateMultiple" value="5"/>
            <flag name="BallPulseRise" value="6"/>
            <flag name="BallRotate" value="7"/>
            <flag name="CubeTransition" value="8"/>
            <flag name="BallZigZag" value="9"/>
            <flag name="BallZigZagDeflect" value="10"/>
            <flag name="BallTrianglePath" value="11"/>
            <flag name="BallScale" value="12"/>
            <flag name="LineScale" value="13"/>
            <flag name="LineScaleParty" value="14"/>
            <flag name="BallScaleMultiple" value="15"/>
            <flag name="BallPulseSync" value="16"/>
            <flag name="BallBeat" value="17"/>
            <flag name="LineScalePulseOut" value="18"/>
            <flag name="LineScalePulseOutRapid" value="19"/>
            <flag name="BallScaleRipple" value="20"/>
            <flag name="BallScaleRippleMultiple" value="21"/>
            <flag name="BallSpinFadeLoader" value="22"/>
            <flag name="LineSpinFadeLoader" value="23"/>
            <flag name="TriangleSkewSpin" value="24"/>
            <flag name="Pacman" value="25"/>
            <flag name="BallGridBeat" value="26"/>
            <flag name="SemiCircleSpin" value="27"/>
        </attr>
        <attr name="indicator_color" format="color"/>
    </declare-styleable>
</resources>
